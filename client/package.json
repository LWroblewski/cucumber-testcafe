    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "testcafe": "^0.20.4",
    "testcafe-angular-selectors": "^0.3.0",
    "ts-node": "~3.2.0",
    "tsickle": ">=0.25.5",
