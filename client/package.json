{
  "name": "ng-bdd-client",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "start:editor": "ng serve bdd-editor --port=4033",
    "start:demo": "ng serve bdd-demo --configuration=mock",
    "build:bdd-editor": "ng build bdd-editor --configuration=production",
    "build:bdd-launcher": "tsc -p ./projects/bdd-launcher/tsconfig.lib.json && cp ./projects/bdd-launcher/package.json ./projects/dist-bdd-launcher && npm pack ./projects/bdd-launcher/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e:demo": "./node_modules/.bin/cucumber-js ./projects/bdd-demo-e2e/**/*.feature --require ./projects/dist-bdd-launcher/lib/support/**/*.js --require ./projects/dist-bdd-launcher/lib/step_definitions/**/*.js --format json:./projects/bdd-demo-e2e/reports/report.json",
    "report-e2e:demo": "node ./projects/dist-bdd-launcher/lib/report/report.js --jsonFile ./projects/bdd-demo-e2e/reports/report.json --output ./projects/bdd-demo-e2e/reports/report.html"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "6.0.7",
    "@angular/core": "6.0.7",
    "@angular/compiler": "6.0.7",
    "@angular/animations": "6.0.7",
    "@angular/cdk": "6.3.1",
    "@angular/forms": "6.0.7",
    "@angular/http": "6.0.7",
    "@angular/material": "6.3.1",
    "@angular/platform-browser": "6.0.7",
    "@angular/platform-browser-dynamic": "6.0.7",
    "@angular/router": "6.0.7",
    "@ngxs/devtools-plugin": "^3.2.0",
    "@ngxs/logger-plugin": "^3.1.4-dev.master-b50211a",
    "@ngxs/router-plugin": "^3.2.0",
    "@ngxs/store": "^3.1.4-dev.master-b50211a",
    "core-js": "^2.4.1",
    "file-saver": "^1.3.8",
    "base64-img": "^1.0.4",
    "rxjs": "^6.2.1",
    "yargs": "12.0.2",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/core": "^0.7.0",
    "@angular-devkit/build-angular": "^0.7.0",
    "@angular/cli": "6.0.8",
    "@angular/compiler-cli": "6.0.7",
    "@angular/language-service": "6.0.7",
    "@types/cucumber": "^4.0.4",
    "@types/file-saver": "^1.3.0",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^6.0.117",
