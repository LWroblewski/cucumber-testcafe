{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ng-bdd-editor": {
      "root": "projects/ng-bdd-editor",
      "sourceRoot": "projects/ng-bdd-editor/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "../ng-bdd-dist/client",
            "index": "projects/ng-bdd-editor/src/index.html",
            "main": "projects/ng-bdd-editor/src/main.ts",
            "tsConfig": "projects/ng-bdd-editor/src/tsconfig.app.json",
            "polyfills": "projects/ng-bdd-editor/src/polyfills.ts",
            "assets": [
              "projects/ng-bdd-editor/src/assets",
              "projects/ng-bdd-editor/src/favicon.ico"
            ],
            "styles": [
              "projects/ng-bdd-editor/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "dev": {
              "fileReplacements": [
                {
                  "replace": "projects/ng-bdd-editor/src/environments/environment.ts",
                  "with": "projects/ng-bdd-editor/src/environments/environment.dev.ts"
                }
              ]
            },
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": true,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "projects/ng-bdd-editor/src/environments/environment.ts",
                  "with": "projects/ng-bdd-editor/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ng-bdd-editor:build"
          },
          "configurations": {
            "prod": {
              "browserTarget": "ng-bdd-editor:build:prod"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ng-bdd-editor:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ng-bdd-editor/src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "projects/ng-bdd-editor/src/polyfills.ts",
            "tsConfig": "projects/ng-bdd-editor/src/tsconfig.spec.json",
            "scripts": [],
            "styles": [
              "projects/ng-bdd-editor/src/styles.scss"
            ],
            "assets": [
              "projects/ng-bdd-editor/src/assets",
              "projects/ng-bdd-editor/src/favicon.ico"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ng-bdd-editor/src/tsconfig.app.json",
              "projects/ng-bdd-editor/src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "ng-bdd": {
      "root": "projects/ng-bdd",
      "sourceRoot": "projects/ng-bdd/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/ng-bdd/tsconfig.lib.json",
            "project": "projects/ng-bdd/ng-package.json"
          },
          "configurations": {
            "production": {
              "project": "projects/ng-bdd/ng-package.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ng-bdd/src/test.ts",
            "tsConfig": "projects/ng-bdd/tsconfig.spec.json",
            "karmaConfig": "projects/ng-bdd/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ng-bdd/tsconfig.lib.json",
              "projects/ng-bdd/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "ng-bdd-editor",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "styleext": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  }
}
